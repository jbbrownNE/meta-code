def getWrongAnswers(N, C):
    wrong_answers = ''
    for i in range(N):
        if C[i] == 'A':
            wrong_answers += 'B'
        else:
            wrong_answers += 'A'
    return wrong_answers
  
  result = getWrongAnswers(3, "ABA")
print(result)  # This will print 'BAB' ended up printing ABA
